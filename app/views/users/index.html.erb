
<h1 class='d-flex justify-content-between'>
  <span>Select a user to share this notebook with</span>
</h1>

<table class='table'>
    <thead>
    <tr>
      <th>Emails:</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  
  <tbody>
  <% @users.each do |user| %>
  <tr>
    <td><%= link_to user.email, "/notebook_users?user_id=#{user.id}&notebook_id=#{@notebookId}" , method: 'POST'%></td>
    </tr>
    <% end %>
  </tbody>
</table>


